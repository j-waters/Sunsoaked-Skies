{
  "name": "sunsoaked-skies",
  "version": "1.0.2",
  "description": "Phaser 3 TypeScript Starter",
  "main": "src/game.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nollup -c --hot --content-base public",
    "dev2": "rollup --config rollup2.config.js",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "start": "serve dist",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geocine/phaser3-rollup-typescript.git"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.0",
    "cross-env": "^7.0.2",
    "nollup": "^0.10.7",
    "rimraf": "^3.0.2",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-static-files": "^0.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "serve": "^11.3.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "patch-package": "^6.2.2",
    "phaser": "^3.23.0",
    "prettier": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1"
  }
}
